scalar Void

enum Role {
  ADMIN
  USER
}

interface UserRequest {
    name: String!
    age: Int!
    enabled: Boolean!
    role: Role!
}

type User implements UserRequest {
    id: Int!
    name: String!
    age: Int!
    enabled: Boolean!
    role: Role!
}

type Queries {
    list: [User!]!
}

type Mutations {
    delete(id: Int!): Void
}

schema {
    query: Queries
    mutation: Mutations
}
