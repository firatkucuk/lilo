scalar Void

type User {
    id: Int!
    name: String!
    age: Int!
    enabled: Boolean!
}

type Queries {
    get(id: Int!): User!
    list: [User!]!
}

type Mutations {
    create(name: String!, age: Int!, enabled: Boolean!): User!
    delete(id: Int!): Void
}

schema {
    query: Queries
    mutation: Mutations
}
